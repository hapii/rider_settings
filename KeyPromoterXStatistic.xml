<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Back">
        <Item>
          <IdeaActionID>Back</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Left'</ShortCut>
          <Description>Back</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'Ctrl+B'</ShortCut>
          <Description>Declaration</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Properties...">
        <Item>
          <IdeaActionID>EditPropertiesAction</IdeaActionID>
          <ShortCut>'Alt+Enter'</ShortCut>
          <Description>Properties...</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'Services'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'Shift+F10'</ShortCut>
          <Description>Run 'Services'</Description>
          <Count>9</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'UI'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'Shift+F10'</ShortCut>
          <Description>Run 'UI'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Solution or Project...">
        <Item>
          <IdeaActionID>RiderOpenSolution</IdeaActionID>
          <ShortCut>'Ctrl+Shift+O'</ShortCut>
          <Description>Solution or Project...</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'Services'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'Ctrl+F2'</ShortCut>
          <Description>Stop 'Services'</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Version Control">
        <Item>
          <IdeaActionID>ActivateVersionControlToolWindow</IdeaActionID>
          <ShortCut>'Alt+9' or 'Alt+Shift+9'</ShortCut>
          <Description>Version Control</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>